# shellcheck shell=sh disable=SC1072,SC3043 #source

test_get_token(){
    token="$(x hub token)"
}

test_get_user_info(){
    @assert "$(x hub user)" "=~" "[a-z0-9_]+ [a-z0-9]+"
}

test_logout(){
    @assert true x hub logout
    @assert "$(x hub user)" = ""
    @assert true x hub token "$token"
}

@assert:file:func